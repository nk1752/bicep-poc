{
  "experimentalFeaturesEnabled": {
    "extensibility": true
  },
  "analyzers": {
    "core": {
      "rules": {
        "no-hardcoded-env-urls": {
          "level": "warning"
        },
        "no-hardcoded-location": {
          "level": "warning"
        },
        "no-unused-params": {
          "level": "warning"
        },
        "use-recent-api-versions": {
          "level": "warning"
        },
        "no-unused-vars": {
          "level": "warning"
        },
        "outputs-should-not-contain-secrets": {
          "level": "warning"
        },
        "secure-secrets-in-params": {
          "level": "warning"
        }
      }
    }
  }
}